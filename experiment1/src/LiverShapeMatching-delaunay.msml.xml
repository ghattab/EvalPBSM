<msml:msml xmlns:msml="http://sfb125.de/msml"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://sfb125.de/msml">
    <variables>
        <var name="vol_mesh" value="delaunay-volume.vtk" logical="TriangularSurface"
             physical="file.vtk"/>
        <var name="msml_mesh_shape" value="deformed-surface.stl" />
        <var name="msml_mesh_shape_ref" value="deformed-volumeSurface.vtk" />
        <var name="chargeNum" value="10" />
    </variables>


    <scene>
        <object id="liver">
            <mesh>
                <linearTet id="liver" mesh="${vol_mesh}"/>
            </mesh>

            <sets>
               
            </sets>


            <material>
                <region id="liverMaterial" indices="${bodyToIndexGroup}">
                    <linearElasticMaterial youngModulus="1000" poissonRatio="0.40"/>
                    <mass name="abc" massDensity="1"/>
                </region>
            </material>

            <constraints>
                <constraint>
                   <shapeMatchingConstraint id="shapeConstraint" referenceMesh="${msml_mesh_shape}" charge="${chargeNum}" adaptCharge="1" samplingDistance="2" adaptSamplingDistance="0" adaptActiveTriangles="1" />
                </constraint>

            </constraints>

            <output>
                <displacement id="disp" timestep="1"/>
            </output>
        </object>
    </scene>

    <workflow>
               <ComputeIndicesFromBoxROI id="bodyToIndexGroup" box="60 60 30 273 273 223" mesh="${vol_mesh}"
                               select="points"/>
    </workflow>


    <environment>
        <solver linearSolver="direct" processingUnit="CPUDouble"
                timeIntegration="NewmarkShapeMatching" mass="full"/>
        <simulation>
            <step name="initial" dt="1" iterations="50" gravity="0 0 0"/>
        </simulation>
    </environment>

</msml:msml>

